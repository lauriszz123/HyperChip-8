var x = 0
var y = 0

var result = 0

if( 1 != result ) {
	draw( x, y, 0 )
} else {
	draw( x, y, 1 )
	result = 1
}

x += 6

if( 1 == result ) {
	draw( x, y, 1 )
	result = 10
} else {
	draw( x, y, 0 )
}

x += 6

if( result > 5 ) {
	draw( x, y, 1 )
	result = 0
} else {
	draw( x, y, 0 )
}

x += 6

if( result < 1 ) {
	draw( x, y, 1 )
	draw( x, y + 5, 1 )
} else {
	draw( x, y, 0 )
}